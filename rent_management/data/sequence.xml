<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

<!--        Booking running number-->
        <record id="seq_booking" model="ir.sequence">
            <field name="name">Booking</field>
            <field name="code">booking.booking</field>
            <field name="prefix">BOOKING/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

<!--        Booking payment running number-->
        <record id="seq_booking_payment" model="ir.sequence">
            <field name="name">Booking Payment</field>
            <field name="code">booking.payment</field>
            <field name="prefix">PAYMENT/</field>
            <field name="padding">5</field>
            <field name="number_increment">3</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>